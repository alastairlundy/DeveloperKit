<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>AluminiumTech.DevKit.DeveloperKit</RootNamespace>
    <AssemblyName>AluminiumTech.DevKit.DeveloperKit</AssemblyName>
    <PackageId>aluminiumtechdevkit</PackageId>
    <Title>AluminiumTech.DevKit.DeveloperKit</Title>
    <Authors>AluminiumTech</Authors>
    <Copyright>2021 (c) AluminiumTech</Copyright>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
      <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://gitlab.com/aluminiumtechdevkit/developerkit</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageReleaseNotes>### Changes
* Re-licensed under BSD 3 clause license
* Renamed HashMapV2 to HashMap
* Renamed ``Add()`` method to ``Put()``
* Added Version Extensions class to add extensions to Versions</PackageReleaseNotes>
    <TargetFrameworks>net5.0;netcoreapp3.1;netstandard2.0</TargetFrameworks>
    <LangVersion>default</LangVersion>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
    <FileVersion>1.3.0.0</FileVersion>
    <PackageVersion>1.3.0</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="ObjectManipulation" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
